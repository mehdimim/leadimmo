{
  "name": "leadimmo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "node ./scripts/build.mjs",
    "preview": "wrangler pages dev .vercel/output/static --compatibility-date=2025-10-01",
    "deploy": "wrangler pages deploy .vercel/output/static",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply samui --local",
    "db:migrate:prod": "wrangler d1 migrations apply samui",
    "seed": "node scripts/seed-d1.mjs",
    "test": "vitest run",
    "e2e": "playwright test"
  },
  "dependencies": {
    "@hookform/resolvers": "3.10.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "0.44.6",
    "next": "15.5.2",
    "next-intl": "3.26.5",
    "next-sitemap": "4.2.3",
    "nodemailer": "^7.0.9",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.65.0",
    "resend": "^3.5.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@cloudflare/workers-types": "^4.20251011.0",
    "@playwright/test": "^1.48.2",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "0.31.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.5.2",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.5.4",
    "vercel": "^47.0.4",
    "vitest": "^1.6.0",
    "wrangler": "^4.43.0"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}


